<!-- books show 페이지에서 posts 나열하기 위해 사용하고 있음 -->

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
    <% postable.posts.each do |post| %>
      <tr>
        <td><%= post.user.name %></td>
        <td><%= post.content %></td>
      </tr>
    </tbody>
  <% end %>
</table>
